{
    "name": "fabcar",
    "version": "1.0.0",
    "description": "FabCar application implemented in JavaScript",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive",
        "start": "node index.js",
        "dev": "nodemon index.js"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "fabric-ca-client": "^2.2.4",
        "fabric-network": "^2.2.4",
        "gridfs-stream": "^1.1.1",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.1.1",
        "multer": "^1.4.5-lts.1",
        "node-schedule": "^2.1.1",
        "nodemailer": "^6.9.9",
        "randomstring": "^1.3.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^5.9.0",
        "mocha": "^5.2.0",
        "nodemon": "^3.0.3",
        "nyc": "^14.1.1",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
